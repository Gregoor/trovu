{
  "name": "trovu-web",
  "version": "1.0.0",
  "description": "Trovu web interface.",
  "main": "",
  "scripts": {
    "test": "jest",
    "doc": "rm -r doc ; jsdoc public/js/* --configure jsdoc.json --destination doc",
    "build": "rollup --config --environment INCLUDE_DEPS,BUILD:production",
    "watch": "rollup --config --watch",
    "clean": "rm public/bundle/*.js public/bundle/*.css",
    "sync": "rsync -vr public/* ntro:~/trovu-web/public/",
    "deploy": "npm run clean || true && npm run build && npm run sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trovu/trovu-web.git"
  },
  "keywords": [
    "javascript",
    "shortcuts",
    "social",
    "bootstrap"
  ],
  "author": "Georg Jaehnig",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/trovu/trovu-web/issues"
  },
  "homepage": "https://github.com/trovu/trovu-web#readme",
  "jest": {},
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.19.0",
    "rollup": "^2.18.0",
    "rollup-plugin-css-only": "^2.1.0"
  },
  "dependencies": {
    "awesomplete": "^1.1.5",
    "bootstrap.native": "^3.0.10",
    "countries-list": "^2.5.4",
    "detect-browser": "^5.1.1",
    "js-yaml": "^3.14.0",
    "moment": "^2.26.0"
  }
}
