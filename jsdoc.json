{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-mermaid"
  ],
  "templates": {},
  "opts": {
    "encoding": "utf8",
    "destination": "./dist/public/docs/developers/code",
    "recurse": true,
    "verbose": true
  }
}